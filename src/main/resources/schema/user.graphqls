scalar DateTime

type UserDocument {
    id: ID
    username: String
    password: String
    fullName: String
    gender: String
    dob: DateTime
    email: String
    phone: String
    statusUser: String
    createdAt: String
    updatedAt: String
}

input CreateUserInput {
    username: String!
    password: String!
    fullName: String
    gender: String
    dob: DateTime
    email: String
    phone: String
}

type Mutation {
    createUser(user: CreateUserInput!): UserDocument
}
